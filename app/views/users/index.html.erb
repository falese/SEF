<div class="col-xs-12">
  <div class="table-responsive">
    <h1 class="roboto_font" style="color:red;">Here are the Current Users</h1>
    <table class="table table-hover">

<!--This is a table row with headers  -->
      <tr>
        <th>Id</th>
        <th>Email</th>
        <th>Name</th>
        <th>Bank</th>
        <th>Bank User Name</th>
        <th>Bank Password </th>

      </tr>
      <% @users.each do |user| %>
      <tr>
        <td> <%=user.id %></td>
        <td> <%=user.email %> </td>
        <td> <%=user.user_name%> </td>
        <td><%=user.bank %> </td>
        <td><%=user.bank_user_name %> </td>
        <td><%=user.bank_password %> </td>


        </td>
      <% end %>
    </table>
  </div>
